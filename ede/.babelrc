{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules":false,
        "targets": {
          "browsers": ["last 2 versions", "ie >= 8"]
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    ["@babel/plugin-transform-modules-commonjs",{
    	"loose": true,
    	"modules":true
    }]
  ]
}
